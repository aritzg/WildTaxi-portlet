<?xml version="1.0" encoding="UTF-8"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="net.sareweb.wildtaxi.model.impl.RequestModelImpl">
		<attributes>
			<id name="requestId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="name" />
			<basic name="fromLat" />
			<basic name="fromLng" />
			<basic name="fromAddress" />
			<basic name="toLat" />
			<basic name="toLng" />
			<basic name="toAddress" />
			<basic name="distance" />
			<basic name="beginDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="duration" />
			<basic name="state" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<transient name="primaryKey" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="net.sareweb.wildtaxi.model.impl.RequestImpl" name="Request">
		<table name="wt_Request" />
		<attributes />
	</entity>
</entity-mappings>